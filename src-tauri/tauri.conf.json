{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app-tempero",
  "version": "0.1.0",
  "identifier": "com.tauri-app-tempero.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "home",
        "title": "Temperos da clara",
        "url": "/#/",
        "width": 1100,
        "height": 620,
        "minWidth": 700,
        "minHeight": 500,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
